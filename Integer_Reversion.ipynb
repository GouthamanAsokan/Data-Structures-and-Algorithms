      "source": [
        "def reverse_integer(num):\n",
        "  reversed_n=0\n",
        "  remainder=0\n",
        "  while num>0:\n",
        "    remainder=num%10\n",
        "    num=num//10\n",
        "    print(num)\n",
        "    reversed_n=reversed_n*10+remainder\n",
        "  return reversed_n"
      ],
      "execution_count": 0,
