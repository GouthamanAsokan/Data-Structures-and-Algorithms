      "source": [
        "def is_anagram(str1,str2):\n",
        "  if len(str1)!=len(str2):\n",
        "    return False\n",
        "\n",
        "  str1=sorted(str1)\n",
        "  str2=sorted(str2)\n",
        "\n",
        "  for i in range(len(str1)):\n",
        "    if str1[i]!=str2[i]:\n",
        "      return False\n",
        "  return True\n"
      ],
      "execution_count": 0,
      "outputs": []
    },
